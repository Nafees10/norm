name "norm"
description "simple ORM for D using DDBC"
authors "nafees"
copyright "Copyright Â© 2023-2024, nafees"
license "MIT"
dependency "utils" version="~>0.7.0"
dependency "ddbc" version="~>0.6.0"
configuration "default" {
	targetType "library"
	subConfiguration "ddbc" "full"
}
configuration "mysql" {
	subConfiguration "ddbc" "MySQL"
}
configuration "sqlite" {
	subConfiguration "ddbc" "SQLite"
}
configuration "pgsql" {
	subConfiguration "ddbc" "PGSQL"
}
configuration "test" {
	targetType "executable"
	versions "norm_test"
	subConfiguration "ddbc" "test"
}
